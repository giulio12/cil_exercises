function I_rec = Decompress(I_comp)

d = I_comp.d;

X_rebuilt = I_comp.U_k*I_comp.Z_k;
X_rebuilt = X_rebuilt + I_comp.X_centered;


x_patch = 0;
y_patch = 0;

for i=1:size(X_rebuilt,2)
    I_rec(x_patch*d+1:x_patch*d+d, y_patch*d+1:y_patch*d+d) = vec2mat(X_rebuilt(:,i), d);
   
   
    y_patch = y_patch + 1;
    if((I_comp.pad_height / d) < y_patch)
        y_patch = 0;
        x_patch = x_patch + 1;
        
        if((I_comp.pad_width / d) < x_patch)
            break;
        end
    end
end

size(I_rec)

I_rec(1:I_comp.width, 1:I_comp.height);

imshow(I_rec)

end